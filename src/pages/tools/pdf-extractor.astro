---
import BaseLayout from '../../layouts/BaseLayout.astro';

import Hero from '../../components/Hero.astro';

import PdfExtractor from '../../vue/PdfExtractor.vue';
---

<meta property="og:type" content="product">
<meta property="og:title" content="Pdf Extractor - Free tools to extract the main content from any URL and convert it into clean HTML, ready for use in your favorite CMS.">
<meta property="og:url" content="https://izdrail.com/tools/pdf-extractor/">
<meta property="og:image" content="https://i.imgur.com/XF92B0Q.png">
<meta property="og:description" content="Start curating your content with this free tool that extracts the main content from any URL and converts it into clean HTML, ready for use in your favorite CMS.">
<meta property="product:plural_title" content="">
<meta property="product:price.amount" content="0">
<meta property="product:price.currency" content="GBP">
<meta name="author" content="Stefan â™¥">
<script type="application/ld+json">
    {
        "@context": "https://schema.org/",
        "@type": "Product",
        "name": "Article Extractor",
        "description": "Start curating your content with this free tool that extracts the main content from any URL and converts it into clean HTML, ready for use in your favorite CMS.",
        "review": {
            "@type": "Review",
            "reviewRating": {
                "@type": "Rating",
                "ratingValue": 4,
                "bestRating": 5
            },
            "author": {
                "@type": "Person",
                "name": "Stefan"
            }
        },
        "aggregateRating": {
            "@type": "AggregateRating",
            "ratingValue": 4.4,
            "reviewCount": 89
        }
    }
</script>
<BaseLayout
	title="Pdf Extractor"
	description="Extract pdf content and convert it into clean HTML, ready for use in your favorite CMS."
>
	<div class="stack gap-20">

		<main class="wrapper stack gap-12">
			<Hero
				title="Pdf extractor"
				tagline="Extract pdf content and convert it into clean HTML, ready for use in your favorite CMS."

			/>

			<PdfExtractor client:visible />

        </main>
	</div>
</BaseLayout>
